{% if resource.meta.commits %}
<h3>Change History</h3>
<section id="commit-log">
{% for c in resource.meta.commits %}
<h4><time class="timeago" datetime="{{ c.date }}">{{ c.date }}</time>: <span class="message"> {{ c.msg | escape }}</span> </h4> 
<div class="changeset">
    <pre class="hash">{{c.hash }}</pre>
    <pre class="message">{{ c.msg | escape }}</pre>
    <pre class="diff">{{ c.diff | escape }}</pre>
<hr>
</div>
{% endfor %}
</section>
{% endif %}
